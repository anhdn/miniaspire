{
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "bootstrap": "^4.3.1",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "helmet": "^3.12.1",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "jest": "^24.7.1",
    "lint-staged": "^9.0.0",
    "next": "^9.3.2",
    "next-redux-wrapper": "^2.1.0",
    "next-routes": "^1.4.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.3",
    "react-test-renderer": "16.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "name": "nextjs-redux-starter",
  "version": "1.0.0",
  "main": "server/index.js",
  "scripts": {
    "lint-staged": "lint-staged",
    "build_pro": "export NODE_ENV=production HOST=https://https://weaspire.sg/ && next build",
    "start": "NODE_ENV=production && node server/index.js",
    "build": "next build",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "dev": "nodemon -w server server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "lint": "standard --verbose | snazzy",
    "now-start": "yarn serve",
    "lint:fix": "standard --fix --verbose | snazzy"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 8.12"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "@testing-library/react/cleanup-after-each"
    ]
  },
  "description": "NextJs, Express, Redux, SASS, Styled-Component and Jest.",
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0"
  }
}
